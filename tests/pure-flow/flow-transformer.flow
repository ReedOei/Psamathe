transformer newAccount(a : any nat) -> res : ! list ! { name : any nat, balance : any nat } {
    [ ! { name : any nat, balance : any nat } ;
        {
            name : any nat |-> a,
            balance : any nat |-> 0
        }
    ] --> res
}

;

[ any nat ; 0,1,2 ] --> newAccount() --> var accounts : list ! { name : any nat, balance : any nat }

