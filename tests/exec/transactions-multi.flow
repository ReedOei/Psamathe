contract Main {
    balance : ! nat

    transaction add(n : ! nat):
        n --> this.balance

    transaction remove(m : ! nat) returns ! nat:
        var t : ! nat := 0
        this.balance --[ m ]-> t
        return t
}

;

var x : empty Main

new Main() --> x

x.add(25)
x.add(75)
x.remove(60)

