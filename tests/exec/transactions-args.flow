contract Main {
    transaction foo(n : ! nat) returns ! nat:
        var t : ! nat := 0
        n --> t
        12 --> t
        return t

    transaction add(n : ! nat, m : ! nat) returns ! nat:
        var t : ! nat := 0
        n --> t
        m --> t
        return t
}

;

var x : ! Main := new Main()
var a : ! nat := 0
var b : ! nat := 0

781 --> a
611 --> b

var res : ! nat := x.add(a,b)
if res != 1392 { revert("Failure!") }

