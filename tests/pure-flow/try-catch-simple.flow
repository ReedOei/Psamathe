transformer idBool(a : ! bool) -> res : ! bool {
    copy(a) --> res
}

transformer notBool(a : ! bool) -> res : ! bool {
    try {
        copy(a)[! such that idBool()] --> var y : bool
        false --> res
    } catch {
        true --> res
    }
}

;

false --> var b : bool
b[! such that notBool() ] --> var c : bool

