contract Main {
    transaction foo(n : nat) returns nat:
        var t : nat
        n --> t
        12 --> t
        return t

    transaction add(n : nat, m : nat) returns nat:
        var t : nat
        n --> t
        m --> t
        return t
}

;

var x : Main
var a : nat
var b : nat

new Main() --> x
781 --> a
611 --> b

var res : nat
x.add(a,b)
x.add --> res

