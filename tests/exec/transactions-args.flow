contract Main {
    transaction foo(n : ! nat) returns ! nat:
        var t : ! nat := 0
        n --> t
        12 --> t
        return t

    transaction add(n : ! nat, m : ! nat) returns ! nat:
        var t : ! nat := 0
        n --> t
        m --> t
        return t
}

;

var x : empty Main
var a : ! nat := 0
var b : ! nat := 0

new Main() --> x
781 --> a
611 --> b

var res : ! nat := 0
x.add(a,b)
x.add --> res

